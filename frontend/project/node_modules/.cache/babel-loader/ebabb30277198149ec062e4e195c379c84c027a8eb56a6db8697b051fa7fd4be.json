ning('无效的商品ID');\n        return;\n      }\n      try {\n        await productApi.deleteProduct(productId);\n        ElMessage.success('商品删除成功');\n        await loadProducts();\n      } catch (error) {\n        ElMessage.error(`删除失败: ${error.message}`);\n        console.error('删除商品错误:', error);\n      }\n    };\n    const loadProducts = async () => {\n      try {\n        if (!merchantId.value) {\n          ElMessage.warning('未获取到商家ID');\n          return;\n        }\n        const response = await productApi.getMerchantProducts(merchantId.value);\n        products.value = response.data || [];\n      } catch (error) {\n        ElMessage.error('加载商品失败');\n        console.error('加载商品错误:', error);\n      }\n    };\n    onMounted(() => {\n      loadProducts();\n    });\n    const __returned__ = {\n      productFilter,\n      products,\n      productDialogVisible,\n      currentProduct,\n      isEditing,\n      merchantId,\n      filteredProducts,\n      dialogTitle,\n      showAddDialog,\n      editProduct,\n      handleImageChange,\n      removeImage,\n      saveProduct,\n      deleteProduct,\n      loadProducts,\n      ref,\n      computed,\n      onMounted,\n      get ElMessage() {\n        return ElMessage;\n      },\n      get productApi() {\n        return productApi;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","ElMessage","productApi","productFilter","keyword","products","productDialogVisible","currentProduct","productName","categoryId","price","stock","description","images","isEditing","merchantId","localStorage","getItem","filteredProducts","value","filter","p","toLowerCase","includes","map","Number","Math","max","dialogTitle","showAddDialog","editProduct","product","productId","warning","handleImageChange","file","rawFile","raw","reader","FileReader","onload","e","push","target","r