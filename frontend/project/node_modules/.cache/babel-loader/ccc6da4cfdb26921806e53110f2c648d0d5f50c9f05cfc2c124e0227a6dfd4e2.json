{"ast":null,"code":"import { ref, reactive, computed, onMounted } from 'vue';\nimport { ElMessage } from 'element-plus';\n\n// 响应式数据 -------------------------------------------------\n\nexport default {\n  __name: 'UserProfile',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const userData = ref({\n      username: '',\n      address: ''\n    });\n    const showDialog = ref(false);\n    const isSubmitting = ref(false);\n    const formRef = ref();\n\n    // 表单数据初始化\n    const editForm = reactive({\n      username: '',\n      address: ''\n    });\n\n    // 验证规则\n    const formRules = {\n      username: [{\n        required: true,\n        message: '用户名不能为空',\n        trigger: 'blur'\n      }, {\n        min: 2,\n        max: 20,\n        message: '长度需在2-20个字符之间',\n        trigger: 'blur'\n      }],\n      address: [{\n        required: true,\n        message: '收货地址不能为空',\n        trigger: 'blur'\n      }, {\n        max: 100,\n        message: '最多可输入100个字符',\n        trigger: 'blur'\n      }]\n    };\n\n    // 计算属性 -------------------------------------------------\n    const currentUsername = computed(() => userData.value.username || '未设置');\n    const currentAddress = computed(() => userData.value.address || '暂无收货地址');\n\n    // 生命周期钩子 ---------------------------------------------\n    onMounted(() => {\n      loadLocalData();\n    });\n\n    // 方法实现 -------------------------------------------------\n    // 加载本地数据\n    const loadLocalData = () => {\n      try {\n        const savedData = localStorage.getItem('userProfile');\n        if (savedData) {\n          const parsedData = JSON.parse(savedData);\n          userData.value.username = parsedData.username || '';\n          userData.value.address = parsedData.address || '';\n        }\n      } catch (error) {\n        console.error('本地数据加载失败:', error);\n        ElMessage.error('个人资料加载失败，已重置');\n        resetLocalData();\n      }\n    };\n\n    // 打开编辑对话框\n    const openEditDialog = () => {\n      // 确保每次打开对话框都重新初始化表单数据\n      editForm.username = userData.value.username;\n      editForm.address = userData.value.address;\n      showDialog.value = true;\n\n      // 清除之前的验证状态\n      if (formRef.value) {\n        formRef.value.clearValidate();\n      }\n    };\n\n    // 提交表单\n    const submitForm = async () => {\n      try {\n        isSubmitting.value = true;\n\n        // 执行表单验证\n        await formRef.value.validate();\n\n        // 更新用户数据\n        userData.value.username = editForm.username.trim();\n        userData.value.address = editForm.address.trim();\n\n        // 保存到本地存储\n        saveToLocalStorage();\n        ElMessage.success('修改成功');\n        showDialog.value = false;\n      } catch (error) {\n        // 验证失败时不处理\n        if (error?.fields) return;\n        ElMessage.error('保存失败，请稍后重试');\n        console.error('保存错误:', error);\n      } finally {\n        isSubmitting.value = false;\n      }\n    };\n\n    // 本地存储操作\n    const saveToLocalStorage = () => {\n      try {\n        const saveData = {\n          username: userData.value.username,\n          address: userData.value.address\n        };\n        localStorage.setItem('userProfile', JSON.stringify(saveData));\n      } catch (error) {\n        console.error('本地存储失败:', error);\n        throw new Error('本地存储失败');\n      }\n    };\n\n    // 重置本地数据\n    const resetLocalData = () => {\n      localStorage.removeItem('userProfile');\n      userData.value = {\n        username: '',\n        address: ''\n      };\n    };\n    const __returned__ = {\n      userData,\n      showDialog,\n      isSubmitting,\n      formRef,\n      editForm,\n      formRules,\n      currentUsername,\n      currentAddress,\n      loadLocalData,\n      openEditDialog,\n      submitForm,\n      saveToLocalStorage,\n      resetLocalData,\n      ref,\n      reactive,\n      computed,\n      onMounted,\n      get ElMessage() {\n        return ElMessage;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","onMounted","ElMessage","userData","username","address","showDialog","isSubmitting","formRef","editForm","formRules","required","message","trigger","min","max","currentUsername","value","currentAddress","loadLocalData","savedData","localStorage","getItem","parsedData","JSON","parse","error","console","resetLocalData","openEditDialog","clearValidate","submitForm","validate","trim","saveToLocalStorage","success","fields","saveData","setItem","stringify","Error","removeItem"],"sources":["E:/实验例子/project/src/components/UserProfile.vue"],"sourcesContent":["<template>\r\n  <div class=\"user-profile\">\r\n    <h2>我的信息</h2>\r\n    \r\n    <!-- 信息展示区域 -->\r\n    <div class=\"info-card\">\r\n      <div class=\"info-item\">\r\n        <label>用户名：</label>\r\n        <span class=\"info-content\">{{ currentUsername }}</span>\r\n      </div>\r\n      <div class=\"info-item\">\r\n        <label>收货地址：</label>\r\n        <span class=\"info-content\">{{ currentAddress }}</span>\r\n      </div>\r\n      <el-button \r\n        type=\"primary\" \r\n        @click=\"openEditDialog\"\r\n        class=\"edit-button\"\r\n      >\r\n        编辑信息\r\n      </el-button>\r\n    </div>\r\n\r\n    <!-- 编辑对话框 -->\r\n    <el-dialog\r\n      v-model=\"showDialog\"\r\n      title=\"修改个人信息\"\r\n      width=\"90%\"\r\n      :max-width=\"500\"\r\n    >\r\n      <el-form\r\n        ref=\"formRef\"\r\n        :model=\"editForm\"\r\n        :rules=\"formRules\"\r\n        label-width=\"100px\"\r\n      >\r\n        <el-form-item label=\"用户名：\" prop=\"username\">\r\n          <el-input\r\n            v-model=\"editForm.username\"\r\n            placeholder=\"请输入2-20位用户名\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n\r\n        <el-form-item label=\"收货地址：\" prop=\"address\">\r\n          <el-input\r\n            v-model=\"editForm.address\"\r\n            type=\"textarea\"\r\n            :rows=\"3\"\r\n            placeholder=\"请输入详细收货地址\"\r\n            maxlength=\"100\"\r\n            show-word-limit\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n\r\n      <template #footer>\r\n        <div class=\"dialog-footer\">\r\n          <el-button @click=\"showDialog = false\">取消</el-button>\r\n          <el-button \r\n            type=\"primary\"\r\n            @click=\"submitForm\"\r\n            :loading=\"isSubmitting\"\r\n          >\r\n            确认修改\r\n          </el-button>\r\n        </div>\r\n      </template>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, reactive, computed, onMounted } from 'vue'\r\nimport { ElMessage } from 'element-plus'\r\n\r\n// 响应式数据 -------------------------------------------------\r\nconst userData = ref({\r\n  username: '',\r\n  address: ''\r\n})\r\n\r\nconst showDialog = ref(false)\r\nconst isSubmitting = ref(false)\r\nconst formRef = ref()\r\n\r\n// 表单数据初始化\r\nconst editForm = reactive({\r\n  username: '',\r\n  address: ''\r\n})\r\n\r\n// 验证规则\r\nconst formRules = {\r\n  username: [\r\n    { required: true, message: '用户名不能为空', trigger: 'blur' },\r\n    { min: 2, max: 20, message: '长度需在2-20个字符之间', trigger: 'blur' }\r\n  ],\r\n  address: [\r\n    { required: true, message: '收货地址不能为空', trigger: 'blur' },\r\n    { max: 100, message: '最多可输入100个字符', trigger: 'blur' }\r\n  ]\r\n}\r\n\r\n// 计算属性 -------------------------------------------------\r\nconst currentUsername = computed(() => \r\n  userData.value.username || '未设置'\r\n)\r\n\r\nconst currentAddress = computed(() =>\r\n  userData.value.address || '暂无收货地址'\r\n)\r\n\r\n// 生命周期钩子 ---------------------------------------------\r\nonMounted(() => {\r\n  loadLocalData()\r\n})\r\n\r\n// 方法实现 -------------------------------------------------\r\n// 加载本地数据\r\nconst loadLocalData = () => {\r\n  try {\r\n    const savedData = localStorage.getItem('userProfile')\r\n    if (savedData) {\r\n      const parsedData = JSON.parse(savedData)\r\n      userData.value.username = parsedData.username || ''\r\n      userData.value.address = parsedData.address || ''\r\n    }\r\n  } catch (error) {\r\n    console.error('本地数据加载失败:', error)\r\n    ElMessage.error('个人资料加载失败，已重置')\r\n    resetLocalData()\r\n  }\r\n}\r\n\r\n// 打开编辑对话框\r\nconst openEditDialog = () => {\r\n  // 确保每次打开对话框都重新初始化表单数据\r\n  editForm.username = userData.value.username\r\n  editForm.address = userData.value.address\r\n  showDialog.value = true\r\n  \r\n  // 清除之前的验证状态\r\n  if (formRef.value) {\r\n    formRef.value.clearValidate()\r\n  }\r\n}\r\n\r\n// 提交表单\r\nconst submitForm = async () => {\r\n  try {\r\n    isSubmitting.value = true\r\n    \r\n    // 执行表单验证\r\n    await formRef.value.validate()\r\n    \r\n    // 更新用户数据\r\n    userData.value.username = editForm.username.trim()\r\n    userData.value.address = editForm.address.trim()\r\n    \r\n    // 保存到本地存储\r\n    saveToLocalStorage()\r\n    \r\n    ElMessage.success('修改成功')\r\n    showDialog.value = false\r\n  } catch (error) {\r\n    // 验证失败时不处理\r\n    if (error?.fields) return\r\n    \r\n    ElMessage.error('保存失败，请稍后重试')\r\n    console.error('保存错误:', error)\r\n  } finally {\r\n    isSubmitting.value = false\r\n  }\r\n}\r\n\r\n// 本地存储操作\r\nconst saveToLocalStorage = () => {\r\n  try {\r\n    const saveData = {\r\n      username: userData.value.username,\r\n      address: userData.value.address\r\n    }\r\n    localStorage.setItem('userProfile', JSON.stringify(saveData))\r\n  } catch (error) {\r\n    console.error('本地存储失败:', error)\r\n    throw new Error('本地存储失败')\r\n  }\r\n}\r\n\r\n// 重置本地数据\r\nconst resetLocalData = () => {\r\n  localStorage.removeItem('userProfile')\r\n  userData.value = { username: '', address: '' }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile {\r\n  padding: 20px;\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.info-card {\r\n  background: #fff;\r\n  padding: 24px;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 12px rgba(0,0,0,0.1);\r\n}\r\n\r\n.info-item {\r\n  display: flex;\r\n  align-items: flex-start;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.info-item label {\r\n  font-weight: 500;\r\n  color: #606266;\r\n  width: 100px;\r\n  flex-shrink: 0;\r\n}\r\n\r\n.info-content {\r\n  flex: 1;\r\n  word-break: break-word;\r\n  color: #303133;\r\n}\r\n\r\n.edit-button {\r\n  margin-top: 20px;\r\n  width: 100%;\r\n  max-width: 200px;\r\n}\r\n\r\n.dialog-footer {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  gap: 12px;\r\n}\r\n\r\n/* 适配移动端 */\r\n@media (max-width: 768px) {\r\n  .info-item {\r\n    flex-direction: column;\r\n    gap: 8px;\r\n  }\r\n  \r\n  .info-item label {\r\n    width: auto;\r\n  }\r\n  \r\n  .edit-button {\r\n    max-width: none;\r\n  }\r\n}\r\n</style>"],"mappings":"AAyEA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AACxD,SAASC,SAAS,QAAQ,cAAc;;AAExC;;;;;;;;IACA,MAAMC,QAAQ,GAAGL,GAAG,CAAC;MACnBM,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC,CAAC;IAEF,MAAMC,UAAU,GAAGR,GAAG,CAAC,KAAK,CAAC;IAC7B,MAAMS,YAAY,GAAGT,GAAG,CAAC,KAAK,CAAC;IAC/B,MAAMU,OAAO,GAAGV,GAAG,CAAC,CAAC;;IAErB;IACA,MAAMW,QAAQ,GAAGV,QAAQ,CAAC;MACxBK,QAAQ,EAAE,EAAE;MACZC,OAAO,EAAE;IACX,CAAC,CAAC;;IAEF;IACA,MAAMK,SAAS,GAAG;MAChBN,QAAQ,EAAE,CACR;QAAEO,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,SAAS;QAAEC,OAAO,EAAE;MAAO,CAAC,EACvD;QAAEC,GAAG,EAAE,CAAC;QAAEC,GAAG,EAAE,EAAE;QAAEH,OAAO,EAAE,eAAe;QAAEC,OAAO,EAAE;MAAO,CAAC,CAC/D;MACDR,OAAO,EAAE,CACP;QAAEM,QAAQ,EAAE,IAAI;QAAEC,OAAO,EAAE,UAAU;QAAEC,OAAO,EAAE;MAAO,CAAC,EACxD;QAAEE,GAAG,EAAE,GAAG;QAAEH,OAAO,EAAE,aAAa;QAAEC,OAAO,EAAE;MAAO,CAAC;IAEzD,CAAC;;IAED;IACA,MAAMG,eAAe,GAAGhB,QAAQ,CAAC,MAC/BG,QAAQ,CAACc,KAAK,CAACb,QAAQ,IAAI,KAC7B,CAAC;IAED,MAAMc,cAAc,GAAGlB,QAAQ,CAAC,MAC9BG,QAAQ,CAACc,KAAK,CAACZ,OAAO,IAAI,QAC5B,CAAC;;IAED;IACAJ,SAAS,CAAC,MAAM;MACdkB,aAAa,CAAC,CAAC;IACjB,CAAC,CAAC;;IAEF;IACA;IACA,MAAMA,aAAa,GAAGA,CAAA,KAAM;MAC1B,IAAI;QACF,MAAMC,SAAS,GAAGC,YAAY,CAACC,OAAO,CAAC,aAAa,CAAC;QACrD,IAAIF,SAAS,EAAE;UACb,MAAMG,UAAU,GAAGC