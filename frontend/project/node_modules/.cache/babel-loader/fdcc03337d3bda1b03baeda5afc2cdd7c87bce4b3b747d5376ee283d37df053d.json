{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, computed } from 'vue';\nimport { useRouter } from 'vue-router';\nexport default {\n  __name: 'ProductList',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n\n    // 商品数据\n    const products = ref([{\n      id: 1,\n      name: '男士运动鞋',\n      price: 299.00,\n      category: '鞋类',\n      image: 'https://example.com/shoe.jpg',\n      stock: 100\n    }\n    // 更多商品数据...\n    ]);\n\n    // 分类数据\n    const categories = ref([{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'clothes',\n      name: '服装'\n    }, {\n      id: 'shoes',\n      name: '鞋类'\n    }]);\n    const selectedCategory = ref('all');\n\n    // 过滤商品\n    const filteredProducts = computed(() => {\n      return selectedCategory.value === 'all' ? products.value : products.value.filter(p => p.category === selectedCategory.value);\n    });\n\n    // 查看详情\n    const viewDetail = productId => {\n      router.push(`/buyer/product/${productId}`);\n    };\n    const __returned__ = {\n      router,\n      products,\n      categories,\n      selectedCategory,\n      filteredProducts,\n      viewDetail,\n      ref,\n      computed,\n      get useRouter() {\n        return useRouter;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useRouter","router","products","id","name","price","category","image","stock","categories","selectedCategory","filteredProducts","value","filter","p","viewDetail","productId","push"],"sources":["E:/实验例子/project/src/views/Buyer/ProductList.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-list\">\r\n    <!-- 分类筛选 -->\r\n    <div class=\"filter-section\">\r\n      <el-select v-model=\"selectedCategory\" placeholder=\"全部分类\">\r\n        <el-option\r\n          v-for=\"category in categories\"\r\n          :key=\"category.id\"\r\n          :label=\"category.name\"\r\n          :value=\"category.id\"\r\n        />\r\n      </el-select>\r\n    </div>\r\n\r\n    <!-- 商品展示 -->\r\n    <div class=\"product-grid\">\r\n      <div \r\n        v-for=\"product in filteredProducts\"\r\n        :key=\"product.id\"\r\n        class=\"product-card\"\r\n        @click=\"viewDetail(product.id)\"\r\n      >\r\n        <el-image :src=\"product.image\" fit=\"cover\" />\r\n        <div class=\"product-info\">\r\n          <h3>{{ product.name }}</h3>\r\n          <p class=\"price\">¥{{ product.price.toFixed(2) }}</p>\r\n          <el-tag type=\"info\">{{ product.category }}</el-tag>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed } from 'vue'\r\nimport { useRouter } from 'vue-router'\r\n\r\nconst router = useRouter()\r\n\r\n// 商品数据\r\nconst products = ref([\r\n  {\r\n    id: 1,\r\n    name: '男士运动鞋',\r\n    price: 299.00,\r\n    category: '鞋类',\r\n    image: 'https://example.com/shoe.jpg',\r\n    stock: 100\r\n  },\r\n  // 更多商品数据...\r\n])\r\n\r\n// 分类数据\r\nconst categories = ref([\r\n  { id: 'all', name: '全部' },\r\n  { id: 'clothes', name: '服装' },\r\n  { id: 'shoes', name: '鞋类' }\r\n])\r\n\r\nconst selectedCategory = ref('all')\r\n\r\n// 过滤商品\r\nconst filteredProducts = computed(() => {\r\n  return selectedCategory.value === 'all' \r\n    ? products.value \r\n    : products.value.filter(p => p.category === selectedCategory.value)\r\n})\r\n\r\n// 查看详情\r\nconst viewDetail = (productId) => {\r\n  router.push(`/buyer/product/${productId}`)\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.product-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(240px, 1fr));\r\n  gap: 20px;\r\n  padding: 20px;\r\n}\r\n\r\n.product-card {\r\n  border: 1px solid #ebeef5;\r\n  border-radius: 4px;\r\n  overflow: hidden;\r\n  cursor: pointer;\r\n  transition: box-shadow 0.3s;\r\n}\r\n\r\n.product-card:hover {\r\n  box-shadow: 0 2px 12px 0 rgba(0,0,0,0.1);\r\n}\r\n\r\n.el-image {\r\n  width: 100%;\r\n  height: 200px;\r\n}\r\n\r\n.product-info {\r\n  padding: 15px;\r\n}\r\n\r\n.price {\r\n  color: #f56c6c;\r\n  font-size: 18px;\r\n  margin: 10px 0;\r\n}\r\n</style>"],"mappings":";;;AAkCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,SAAS,QAAQ,YAAY;;;;;;;IAEtC,MAAMC,MAAM,GAAGD,SAAS,CAAC,CAAC;;IAE1B;IACA,MAAME,QAAQ,GAAGJ,GAAG,CAAC,CACnB;MACEK,EAAE,EAAE,CAAC;MACLC,IAAI,EAAE,OAAO;MACbC,KAAK,EAAE,MAAM;MACbC,QAAQ,EAAE,IAAI;MACdC,KAAK,EAAE,8BAA8B;MACrCC,KAAK,EAAE;IACT;IACA;IAAA,CACD,CAAC;;IAEF;IACA,MAAMC,UAAU,GAAGX,GAAG,CAAC,CACrB;MAAEK,EAAE,EAAE,KAAK;MAAEC,IAAI,EAAE;IAAK,CAAC,EACzB;MAAED,EAAE,EAAE,SAAS;MAAEC,IAAI,EAAE;IAAK,CAAC,EAC7B;MAAED,EAAE,EAAE,OAAO;MAAEC,IAAI,EAAE;IAAK,CAAC,CAC5B,CAAC;IAEF,MAAMM,gBAAgB,GAAGZ,GAAG,CAAC,KAAK,CAAC;;IAEnC;IACA,MAAMa,gBAAgB,GAAGZ,QAAQ,CAAC,MAAM;MACtC,OAAOW,gBAAgB,CAACE,KAAK,KAAK,KAAK,GACnCV,QAAQ,CAACU,KAAK,GACdV,QAAQ,CAACU,KAAK,CAACC,MAAM,CAACC,CAAC,IAAIA,CAAC,CAACR,QAAQ,KAAKI,gBAAgB,CAACE,KAAK,CAAC;IACvE,CAAC,CAAC;;IAEF;IACA,MAAMG,UAAU,GAAIC,SAAS,IAAK;MAChCf,MAAM,CAACgB,IAAI,CAAC,kBAAkBD,SAAS,EAAE,CAAC;IAC5C,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}