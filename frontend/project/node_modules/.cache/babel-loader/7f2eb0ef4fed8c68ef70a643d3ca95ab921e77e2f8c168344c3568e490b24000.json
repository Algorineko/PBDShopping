{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { useCartStore } from '@/stores/cart';\nexport default {\n  __name: 'ProductDetail',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const cartStore = useCartStore();\n\n    // 图片URL处理\n    const formatImageUrl = url => {\n      if (!url) return '';\n      return url.split('?')[0];\n    };\n    const product = ref({\n      id: '',\n      name: '',\n      price: 0,\n      inventory: 0,\n      salesCount: 0,\n      mainPictures: [],\n      detailDesc: ''\n    });\n    const quantity = ref(1);\n    const fetchProductDetail = async id => {\n      try {\n        const response = await fetch(`http://pcapi-xiaotuxian-front-devtest.itheima.net/goods?id=${id}`);\n        const res = await response.json();\n        if (res.code !== '1') throw new Error(res.msg || '商品加载失败');\n\n        // 数据结构映射\n        product.value = {\n          id: res.result.id,\n          name: res.result.name,\n          price: Number(res.result.price).toFixed(2),\n          inventory: res.result.inventory || 0,\n          salesCount: res.result.salesCount || 0,\n          mainPictures: (res.result.mainPictures || []).map(formatImageUrl),\n          detailDesc: res.result.details?.properties?.[0]?.value || ''\n        };\n      } catch (error) {\n        ElMessage.error(error.message);\n      }\n    };\n    const addToCart = () => {\n      cartStore.addItem({\n        id: product.value.id,\n        name: product.value.name,\n        price: product.value.price,\n        quantity: quantity.value,\n        image: product.value.mainPictures[0]\n      });\n      ElMessage.success('已加入购物车');\n    };\n    onMounted(() => {\n      const productId = route.params.id;\n      if (productId) fetchProductDetail(productId);\n    });\n    const __returned__ = {\n      route,\n      cartStore,\n      formatImageUrl,\n      product,\n      quantity,\n      fetchProductDetail,\n      addToCart,\n      ref,\n      onMounted,\n      get useRoute() {\n        return useRoute;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get useCartStore() {\n        return useCartStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRoute","ElMessage","useCartStore","route","cartStore","formatImageUrl","url","split","product","id","name","price","inventory","salesCount","mainPictures","detailDesc","quantity","fetchProductDetail","response","fetch","res","json","code","Error","msg","value","result","Number","toFixed","map","details","properties","error","message","addToCart","addItem","image","success","productId","params"],"sources":["E:/实验例子/project/src/views/Buyer/ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <el-button type=\"text\" @click=\"$router.go(-1)\">返回</el-button>\r\n    \r\n    <div class=\"detail-container\">\r\n      <!-- 左侧图片展示区 -->\r\n      <div class=\"image-section\">\r\n        <el-carousel \r\n          :interval=\"4000\" \r\n          height=\"500px\"\r\n          indicator-position=\"outside\"\r\n        >\r\n          <el-carousel-item \r\n            v-for=\"(img, index) in product.mainPictures\" \r\n            :key=\"index\"\r\n          >\r\n            <el-image \r\n              :src=\"formatImageUrl(img)\" \r\n              fit=\"contain\"\r\n              class=\"detail-image\"\r\n              :preview-src-list=\"product.mainPictures.map(formatImageUrl)\"\r\n            />\r\n          </el-carousel-item>\r\n        </el-carousel>\r\n      </div>\r\n\r\n      <!-- 右侧商品信息 -->\r\n      <div class=\"info-section\">\r\n        <h1 class=\"product-title\">{{ product.name }}</h1>\r\n        <div class=\"price-info\">\r\n          <span class=\"current-price\">¥{{ product.price }}</span>\r\n        </div>\r\n        \r\n        <div class=\"meta-info\">\r\n          <el-tag type=\"success\">库存 {{ product.inventory }} 件</el-tag>\r\n          <el-tag>销量 {{ product.salesCount }}</el-tag>\r\n        </div>\r\n\r\n        <div class=\"action-group\">\r\n          <el-input-number \r\n            v-model=\"quantity\" \r\n            :min=\"1\" \r\n            :max=\"product.inventory\"\r\n            size=\"large\"\r\n          />\r\n          <el-button \r\n            type=\"primary\" \r\n            size=\"large\"\r\n            @click=\"addToCart\"\r\n            :disabled=\"product.inventory === 0\"\r\n          >\r\n            {{ product.inventory > 0 ? '加入购物车' : '已售罄' }}\r\n          </el-button>\r\n        </div>\r\n\r\n        <el-divider />\r\n\r\n        <div class=\"product-desc\">\r\n          <h3>商品详情</h3>\r\n          <p>{{ product.detailDesc || '暂无详情描述' }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { useCartStore } from '@/stores/cart'\r\n\r\nconst route = useRoute()\r\nconst cartStore = useCartStore()\r\n\r\n// 图片URL处理\r\nconst formatImageUrl = (url) => {\r\n  if (!url) return ''\r\n  return url.split('?')[0]\r\n}\r\n\r\nconst product = ref({\r\n  id: '',\r\n  name: '',\r\n  price: 0,\r\n  inventory: 0,\r\n  salesCount: 0,\r\n  mainPictures: [],\r\n  detailDesc: ''\r\n})\r\n\r\nconst quantity = ref(1)\r\n\r\nconst fetchProductDetail = async (id) => {\r\n  try {\r\n    const response = await fetch(\r\n      `http://pcapi-xiaotuxian-front-devtest.itheima.net/goods?id=${id}`\r\n    )\r\n    const res = await response.json()\r\n    if (res.code !== '1') throw new Error(res.msg || '商品加载失败')\r\n    \r\n    // 数据结构映射\r\n    product.value = {\r\n      id: res.result.id,\r\n      name: res.result.name,\r\n      price: Number(res.result.price).toFixed(2),\r\n      inventory: res.result.inventory || 0,\r\n      salesCount: res.result.salesCount || 0,\r\n      mainPictures: (res.result.mainPictures || []).map(formatImageUrl),\r\n      detailDesc: res.result.details?.properties?.[0]?.value || ''\r\n    }\r\n  } catch (error) {\r\n    ElMessage.error(error.message)\r\n  }\r\n}\r\n\r\nconst addToCart = () => {\r\n  cartStore.addItem({\r\n    id: product.value.id,\r\n    name: product.value.name,\r\n    price: product.value.price,\r\n    quantity: quantity.value,\r\n    image: product.value.mainPictures[0]\r\n  })\r\n  ElMessage.success('已加入购物车')\r\n}\r\n\r\nonMounted(() => {\r\n  const productId = route.params.id\r\n  if (productId) fetchProductDetail(productId)\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n.detail-container {\r\n  max-width: 1200px;\r\n  margin: 20px auto;\r\n  padding: 20px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  gap: 40px;\r\n  background: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 12px rgba(0,0,0,0.1);\r\n}\r\n\r\n.image-section {\r\n  padding: 20px;\r\n}\r\n\r\n.detail-image {\r\n  width: 100%;\r\n  height: 500px;\r\n  object-fit: contain;\r\n  background: #f5f7fa;\r\n  border-radius: 8px;\r\n}\r\n\r\n.info-section {\r\n  padding: 20px;\r\n}\r\n\r\n.product-title {\r\n  font-size: 24px;\r\n  margin-bottom: 20px;\r\n  color: #333;\r\n}\r\n\r\n.price-info {\r\n  margin: 20px 0;\r\n}\r\n\r\n.current-price {\r\n  font-size: 28px;\r\n  color: #f56c6c;\r\n  font-weight: bold;\r\n}\r\n\r\n.meta-info {\r\n  margin: 20px 0;\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.action-group {\r\n  margin: 30px 0;\r\n  display: flex;\r\n  gap: 20px;\r\n  align-items: center;\r\n}\r\n\r\n.product-desc {\r\n  line-height: 1.6;\r\n  color: #666;\r\n}\r\n\r\n.el-carousel {\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n}\r