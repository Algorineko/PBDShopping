eShopInfo = (newInfo) => {\r\n  shopInfo.value = { ...shopInfo.value, ...newInfo }\r\n}\r\n\r\nonMounted(() => {\r\n  const userId = localStorage.getItem('userId')\r\n  const userName = localStorage.getItem('userName')\r\n  \r\n  shopInfo.value.id = userId || 'BUS2023VIP'\r\n  shopInfo.value.name = userName || '商家名称'\r\n  \r\n  const businessUsers = JSON.parse(localStorage.getItem('businessUsers') || '[]')\r\n  const currentBusiness = businessUsers.find(u => u.userId === userId)\r\n  \r\n  if (currentBusiness) {\r\n    shopInfo.value = {\r\n      ...shopInfo.value,\r\n      address: currentBusiness.address || '',\r\n      contact: currentBusiness.contact || '',\r\n      avatar: currentBusiness.avatar || ''\r\n    }\r\n  }\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n.business-container {\r\n  display: grid;\r\n  grid-template-columns: 220px 1fr;\r\n  grid-template-rows: 80px 1fr;\r\n  height: 100vh;\r\n  background: #f5f7fa;\r\n  font-family: 'Helvetica Neue', Arial, sans-serif;\r\n  overflow: hidden;\r\n}\r\n\r\n.action-bar {\r\n  grid-column: 1 / -1;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 0 30px;\r\n  background: #fff;\r\n  border-bottom: 1px solid #ebeef5;\r\n  box-shadow: 0 2px 8px rgba(0,0,0,0.05);\r\n  z-index: 10;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px;\r\n}\r\n\r\n.user-details {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.welcome-text {\r\n  font-size: 18px;\r\n  font-weight: 600;\r\n  color: #303133;\r\n}\r\n\r\n.shop-id {\r\n  font-size: 13px;\r\n  color: #909399;\r\n}\r\n\r\n.logout-btn {\r\n  padding: 10px 20px;\r\n  border-radius: 6px;\r\n  font-weight: 500;\r\n}\r\n\r\n.function-panel {\r\n  padding: 25px 0;\r\n  background: #fff;\r\n  border-right: 1px solid #ebeef5;\r\n  overflow-y: auto;\r\n  box-shadow: 2px 0 5px rgba(0,0,0,0.03);\r\n}\r\n\r\n.function-card {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 16px 30px;\r\n  cursor: pointer;\r\n  transition: all 0.3s;\r\n  font-size: 15px;\r\n  color: #606266;\r\n  border-left: 4px solid transparent;\r\n  text-decoration: none;\r\n}\r\n\r\n.function-card:hover {\r\n  background-color: #f5f7fa;\r\n  color: #409eff;\r\n}\r\n\r\n.function-card.active {\r\n  background-color: #ecf5ff;\r\n  color: #409eff;\r\n  font-weight: 500;\r\n  border-left: 4px solid #409eff;\r\n}\r\n\r\n.function-icon {\r\n  margin-right: 15px;\r\n  font-size: 20px;\r\n  width: 24px;\r\n  text-align: center;\r\n}\r\n\r\n.content-area {\r\n  padding: 30px;\r\n  background: white;\r\n  overflow-y: auto;\r\n  max-height: calc(100vh - 80px);\r\n}\r\n</style>"],"mappings":";;EACOA,KAAK,EAAC;AAAoB;;EACxBA,KAAK,EAAC;AAAY;;EAChBA,KAAK,EAAC;AAAW;;EAEfA,KAAK,EAAC;AAAc;;EACjBA,KAAK,EAAC;AAAc;;EACpBA,KAAK,EAAC;AAAS;;EAQtBA,KAAK,EAAC;AAAgB;;EAatBA,KAAK,EAAC;AAAc;;;;;;uBA3B3BC,mBAAA,CA8BM,OA9BNC,UA8BM,GA7BJC,mBAAA,CAWM,OAXNC,UAWM,GAVJD,mBAAA,CAMM,OANNE,UAMM,GALJC,YAAA,CAA+CC,oBAAA;IAAnCC,IAAI,EAAE,EAAE;IAAGC,GAAG,EAAEC,MAAA,CAAAC,QAAQ,CAACC;oCACrCT,mBAAA,CAGM,OAHNU,UAGM,GAFJV,mBAAA,CAAwD,QAAxDW,UAAwD,EAA7B,KAAG,GAAAC,gBAAA,CAAGL,MAAA,CAAAC,QAAQ,CAACK,IAAI,kBAC9Cb,mBAAA,CAAoD,QAApDc,UAAoD,EAA9B,QAAM,GAAAF,gBAAA,CAAGL,MAAA,CAAAC,QAAQ,CAACO,EAAE,iB,KAG9CZ,YAAA,CAEYa,oBAAA;IAFDnB,KAAK,EAAC,YAAY;IAACoB,IAAI,EAAC,QAAQ;IAAEC,OAAK,EAAEX,MAAA,CAAAY;;IAV1DC,OAAA,EAAAC,QAAA,CAWQ,MAAqCC,MAAA,QAAAA,MAAA,OAArCtB,mBAAA,CAAqC;MAAlCH,KAAK,EAAC;IAAuB,4BAXxC0B,gBAAA,CAW6C,QACvC,E;IAZNC,CAAA;IAAAC,EAAA;QAeIzB,mBAAA,CAWM,OAXN0B,UAWM,I,kBAVJ5B,mBAAA,CASc6B,SAAA,QAzBpBC,WAAA,CAiBuBrB,MAAA,CAAAsB,SAAS,EAAjBC,IAAI;yBADbC,YAAA,CAScC,sBAAA;MAPXC,GAAG,EAAEH,IAAI,CAACG,GAAG;MACbC,EAAE;QAAArB,IAAA,EAAUiB,IAAI,CAACK;MAAS;MAC3BtC,KAAK,EApBbuC,eAAA,EAoBc,eAAe;QAAAC,MAAA,EACH9B,MAAA,CAAA+B,QAAQ,CAACR,IAAI,CAACK,SAAS;MAAA;;MArBjDf,OAAA,EAAAC,QAAA,CAuBQ,MAAgD,CAAhDrB,mBAAA,CAAgD;QAA5CH,KAAK,EAvBjBuC,eAAA,EAuBmBN,IAAI,CAACS,IAAI,EAAQ,eAAe;+BAC3CvC,mBAAA,CAA6B,cAAAY,gBAAA,CAApBkB,IAAI,CAACU,KAAK,iB;MAxB3BhB,CAAA