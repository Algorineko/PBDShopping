{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport { ref, onMounted } from 'vue';\nimport { useRoute } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { useCartStore } from '@/stores/cart';\nexport default {\n  __name: 'ProductDetail',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const cartStore = useCartStore();\n    const reviews = ref([]);\n    onMounted(() => {\n      const savedReviews = JSON.parse(localStorage.getItem('productReviews') || '[]');\n      reviews.value = savedReviews.filter(r => r.productId === product.value.id);\n    });\n    const product = ref({\n      id: '',\n      name: '',\n      price: 0,\n      inventory: 0,\n      mainPictures: [],\n      detailDesc: ''\n    });\n    const quantity = ref(1);\n    const fetchProductDetail = async id => {\n      try {\n        const response = await fetch(`http://pcapi-xiaotuxian-front-devtest.itheima.net/goods?id=${id}`);\n        const res = await response.json();\n        if (res.code !== '1') throw new Error(res.msg || '商品加载失败');\n        product.value = {\n          id: res.result.id,\n          name: res.result.name,\n          price: Number(res.result.price).toFixed(2),\n          inventory: res.result.inventory || 0,\n          mainPictures: res.result.mainPictures || [],\n          detailDesc: res.result.details?.properties?.[0]?.value || ''\n        };\n\n        // 根据库存设置初始数量\n        quantity.value = product.value.inventory > 0 ? 1 : 0;\n      } catch (error) {\n        ElMessage.error(error.message);\n      }\n    };\n    onMounted(() => {\n      const productId = route.params.id;\n      if (productId) fetchProductDetail(productId);\n    });\n    const addToCart = () => {\n      cartStore.addItem({\n        id: product.value.id,\n        name: product.value.name,\n        price: product.value.price,\n        quantity: quantity.value,\n        image: product.value.mainPictures[0]\n      });\n      ElMessage.success('已加入购物车');\n    };\n    const __returned__ = {\n      route,\n      cartStore,\n      reviews,\n      product,\n      quantity,\n      fetchProductDetail,\n      addToCart,\n      ref,\n      onMounted,\n      get useRoute() {\n        return useRoute;\n      },\n      get ElMessage() {\n        return ElMessage;\n      },\n      get useCartStore() {\n        return useCartStore;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","useRoute","ElMessage","useCartStore","route","cartStore","reviews","savedReviews","JSON","parse","localStorage","getItem","value","filter","r","productId","product","id","name","price","inventory","mainPictures","detailDesc","quantity","fetchProductDetail","response","fetch","res","json","code","Error","msg","result","Number","toFixed","details","properties","error","message","params","addToCart","addItem","image","success"],"sources":["E:/实验例子/project/src/views/Buyer/ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <el-button type=\"text\" @click=\"$router.go(-1)\">返回</el-button>\r\n    \r\n    <div class=\"detail-content\">\r\n      <el-carousel :interval=\"4000\" height=\"400px\">\r\n        <el-carousel-item \r\n          v-for=\"(img, index) in product.mainPictures\" \r\n          :key=\"index\"\r\n          style=\"height: 400px;\"\r\n        >\r\n          <el-image \r\n            :src=\"img\" \r\n            fit=\"cover\" \r\n            style=\"width: 100%; height: 100%;\"\r\n          />\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n\r\n      <div class=\"product-info\">\r\n        <h1>{{ product.name }}</h1>\r\n        <p class=\"price\">¥{{ product.price }}</p>\r\n        <el-tag type=\"success\">库存 {{ product.inventory }} 件</el-tag>\r\n        \r\n        <div class=\"action-group\">\r\n          <el-input-number \r\n            v-model=\"quantity\" \r\n            :min=\"product.inventory > 0 ? 1 : 0\"\r\n            :max=\"product.inventory\"\r\n            :disabled=\"product.inventory === 0\"\r\n          />\r\n          <el-button \r\n            type=\"primary\" \r\n            @click=\"addToCart\"\r\n            :disabled=\"product.inventory === 0\"\r\n          >\r\n            {{ product.inventory > 0 ? '加入购物车' : '已售罄' }}\r\n          </el-button>\r\n        </div>\r\n\r\n        <el-divider />\r\n        <div class=\"description\">\r\n          <h3>商品详情</h3>\r\n          <p>{{ product.detailDesc || '暂无详情描述' }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { ElMessage } from 'element-plus'\r\nimport { useCartStore } from '@/stores/cart'\r\n\r\n\r\nconst route = useRoute()\r\nconst cartStore = useCartStore()\r\nconst reviews = ref([])\r\n\r\nonMounted(() => {\r\n  const savedReviews = JSON.parse(localStorage.getItem('productReviews') || '[]')\r\n  reviews.value = savedReviews.filter(r => r.productId === product.value.id)\r\n})\r\nconst product = ref({\r\n  id: '',\r\n  name: '',\r\n  price: 0,\r\n  inventory: 0,\r\n  mainPictures: [],\r\n  detailDesc: ''\r\n})\r\n\r\nconst quantity = ref(1)\r\n\r\nconst fetchProductDetail = async (id) => {\r\n  try {\r\n    const response = await fetch(\r\n      `http://pcapi-xiaotuxian-front-devtest.itheima.net/goods?id=${id}`\r\n    )\r\n    const res = await response.json()\r\n    if (res.code !== '1') throw new Error(res.msg || '商品加载失败')\r\n    \r\n    product.value = {\r\n      id: res.result.id,\r\n      name: res.result.name,\r\n      price: Number(res.result.price).toFixed(2),\r\n      inventory: res.result.inventory || 0,\r\n      mainPictures: res.result.mainPictures || [],\r\n      detailDesc: res.result.details?.properties?.[0]?.value || ''\r\n    }\r\n\r\n    // 根据库存设置初始数量\r\n    quantity.value = product.value.inventory > 0 ? 1 : 0\r\n  } catch (error) {\r\n    ElMessage.error(error.message)\r\n  }\r\n}\r\n\r\nonMounted(() => {\r\n  const productId = route.params.id\r\n  if (productId) fetchProductDetail(productId)\r\n})\r\n\r\nconst addToCart = () => {\r\n  cartStore.addItem({\r\n    id: product.value.id,\r\n    name: product.value.name,\r\n    price: product.value.price,\r\n    quantity: quantity.value,\r\n    image: product.value.mainPictures[0]\r\n  })\r\n  ElMessage.success('已加入购物车')\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.product-image {\r\n  width: 100%;\r\n  height: 200px;\r\n  object-fit: cover;\r\n  border-radius: 8px 8px 0 0;\r\n}\r\n\r\n.el-carousel__item {\r\n  background-color: #f5f7fa;\r\n}\r\n\r\n.detail-content {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  gap: 40px;\r\n}\r\n\r\n.action-group {\r\n  margin-top: 30px;\r\n  display: flex;\r\n  gap: 20px;\r\n  align-items: center;\r\n}\r\n\r\n.description {\r\n  margin-top: 30px;\r\n}\r\n.reviews {\r\n  margin-top: 30px;\r\n}\r\n.review-item {\r\n  padding: 15px;\r\n  border-bottom: 1px solid #ebeef5;\r\n}\r\n.review-header {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px;\r\n  margin-bottom: 10px;\r\n}\r\n.date {\r\n  color: #909399;\r\n  font-size: 0.9em;\r\n}\r\n.content {\r\n  color: #606266;\r\n}\r\n</style>"],"mappings":";;AAmDA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,SAASC,QAAQ,QAAQ,YAAY;AACrC,SAASC,SAAS,QAAQ,cAAc;AACxC,SAASC,YAAY,QAAQ,eAAe;;;;;;;IAG5C,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,SAAS,GAAGF,YAAY,CAAC,CAAC;IAChC,MAAMG,OAAO,GAAGP,GAAG,CAAC,EAAE,CAAC;IAEvBC,SAAS,CAAC,MAAM;MACd,MAAMO,YAAY,GAAGC,IAAI,CAACC,KAAK,CAACC,YAAY,CAACC,OAAO,CAAC,gBAAgB,CAAC,IAAI,IAAI,CAAC;MAC/EL,OAAO,CAACM,KAAK,GAAGL,YAAY,CAACM,MAAM,CAACC,CAAC,IAAIA,CAAC,CAACC,SAAS,KAAKC,OAAO,CAACJ,KAAK,CAACK,EAAE,