:/实验例子/project/src/views/Buyer/ProductDetail.vue"],"sourcesContent":["<template>\r\n  <div class=\"product-detail\">\r\n    <el-button type=\"text\" @click=\"$router.go(-1)\">返回</el-button>\r\n    \r\n    <div class=\"detail-content\">\r\n      <!-- 商品图片 -->\r\n      <el-carousel :interval=\"4000\" height=\"400px\">\r\n        <el-carousel-item v-for=\"(img, index) in product.images\" :key=\"index\">\r\n          <el-image :src=\"img\" fit=\"cover\" />\r\n        </el-carousel-item>\r\n      </el-carousel>\r\n\r\n      <!-- 商品信息 -->\r\n      <div class=\"product-info\">\r\n        <h1>{{ product.name }}</h1>\r\n        <p class=\"price\">¥{{ product.price.toFixed(2) }}</p>\r\n        <el-tag type=\"success\">库存 {{ product.stock }} 件</el-tag>\r\n        \r\n        <div class=\"action-group\">\r\n          <el-input-number \r\n            v-model=\"quantity\" \r\n            :min=\"1\" \r\n            :max=\"product.stock\"\r\n          />\r\n          <el-button \r\n            type=\"primary\" \r\n            @click=\"addToCart\"\r\n            :disabled=\"product.stock === 0\"\r\n          >\r\n            {{ product.stock > 0 ? '加入购物车' : '已售罄' }}\r\n          </el-button>\r\n        </div>\r\n\r\n        <!-- 商品描述 -->\r\n        <el-divider />\r\n        <div class=\"description\">\r\n          <h3>商品详情</h3>\r\n          <p>{{ product.description }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ElMessage } from 'element-plus' // 添加这行导入\r\nimport { ref, onMounted } from 'vue'\r\nimport { useRoute } from 'vue-router'\r\nimport { useCartStore } from '@/stores/cart'\r\n\r\nconst route = useRoute()\r\nconst cartStore = useCartStore()\r\n\r\nconst product = ref({\r\n  id: null,\r\n  name: '',\r\n  price: 0,\r\n  stock: 0,\r\n  images: [],\r\n  description: ''\r\n})\r\n\r\nconst quantity = ref(1)\r\n\r\n// 获取商品详情\r\nonMounted(async () => {\r\n  const productId = route.params.ida\r\n  // 这里应调用API获取商品详情\r\n  product.value = {\r\n    id: productId,\r\n    name: '男士运动鞋',\r\n    price: 299.00,\r\n    stock: 100,\r\n    images: [\r\n      'https://example.com/shoe-1.jpg',\r\n      'https://example.com/shoe-2.jpg'\r\n    ],\r\n    description: '采用透气网布材质，轻便缓震设计...'\r\n  }\r\n})\r\n\r\n// 加入购物车\r\nconst addToCart = () => {\r\n  cartStore.addItem({\r\n    id: product.value.id,\r\n    name: product.value.name,\r\n    price: product.value.price,\r\n    quantity: quantity.value,\r\n    image: product.value.images[0]\r\n  })\r\n  ElMessage.success('已加入购物车')\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.detail-content {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  gap: 40px;\r\n}\r\n\r\n.action-group {\r\n  margin-top: 30px;\r\n  display: flex;\r\n  gap: 20px;\r\n  align-items: center;\r\n}\r\n\r\n.description {\r\n  margin-top: 30px;\r\n}\r\n</style>"],"mappings":"AA6CA,SAASA,SAAS,QAAQ,cAAc,EAAC;AACzC,SAASC,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,SAASC,QAAQ,QAAQ,YAAY;AACrC,SAASC,YAAY,QAAQ,eAAe;;;;;;;IAE5C,MAAMC,KAAK,GAAGF,QAAQ,CAAC,CAAC;IACxB,MAAMG,SAAS,GAAGF,YAAY,CAAC,CAAC;IAEhC,MAAMG,OAAO,GAAGN,GAAG,CAAC;MAClBO,EAAE,EAAE,IAAI;MACRC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,CAAC;MACRC,KAAK,EAAE,CAAC;MACRC,MAAM,EAAE,EAAE;MACVC,WAAW,EAAE;IACf,CAAC,CAAC;IAEF,MAAMC,QAAQ,GAAGb,GAAG,CAAC,CAAC,CAAC;;IAEvB;IACAC,SAAS,CAAC,YAAY;MACpB,MAAMa,SAAS,GAAGV,KAAK,CAACW,MAAM,CAACC,GAAG;MAClC;MACAV,OAAO,CAACW,KAAK,GAAG;QACdV,EAAE,EAAEO,SAAS;QACbN,IAAI,EAAE,OAAO;QACbC,KAAK,EAAE,MAAM;QACbC,KAAK,EAAE,GAAG;QACVC,MAAM,EAAE,CACN,gCAAgC,EAChC,gCAAgC,CACjC;QACDC,WAAW,EAAE;MACf,CAAC;IACH,CAAC,CAAC;;IAEF;IACA,MAAMM,SAAS,GAAGA,CAAA,KAAM;MACtBb,SAAS,CAACc,OAAO,CAAC;QAChBZ,EAAE,EAAED,OAAO,CAACW,KAAK,CAACV,EAAE;QACpBC,IAAI,EAAEF,OAAO,CAACW,KAAK,CAACT,IAAI;QACxBC,KAAK,EAAEH,OAAO,CAACW,KAAK,CAACR,KAAK;QAC1BI,QAAQ,EAAEA,QAAQ,CAACI,KAAK;QACxBG,KAAK,EAAEd,OAAO,CAACW,KAAK,CAACN,MAAM,CAAC,CAAC;MAC/B,CAAC,CAAC;MACFZ,SAAS,CAACsB,OAAO,CAAC,QAAQ,CAAC;IAC7B,CAAC